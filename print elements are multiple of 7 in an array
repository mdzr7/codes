package payilagam;

public class array {
    void m() {
        int a[] = {14, 8, 7, 35, 5, 63, 6, 49, 11, 2};

        for (int i = 0; i < a.length; i = i + 1) {
            if (a[i] % 7 == 0){
                System.out.print(a[i]);
                System.out.print("  ");
            }
        }
        System.out.print("those numbers are multiple with 7");
    }

    public static void main(String[] args) {
        array a = new array();
        a.m();
    }
}
