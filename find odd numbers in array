public class array {
    void m() {
        int a[] = {1, 2, 3, 4, 5, 6, 7, 8, 9};
        int b = 1;
        
        for (int i = 0; i < a.length; i = i + 1) {
            if (a[i] == b) {
                b = b + 2;
                System.out.println(a[i]);
            }
        }
    }

    public static void main(String[] args) {
        array a = new array();
        a.m();
    }
}
