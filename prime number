public class prime_number {
    public static void main(String[] args) {
        int number = 10;
        boolean flag = false;
        for (int i = 2; i <= number / 2; i = i + 1) {

            if (number % i == 0) {
                flag = true;
                break;
            }

        }
        if (flag) {
            System.out.println("not prime number");
        } else
            System.out.println("prime prime number");
    }
}
