package payilagam;

public class array {
    void m() {
        int a[] = {6, 0, 7, 3, 5, 9, 3, 4, 9, 2};
        int b = 1;

        for (int i = 0; i < a.length; i = i + 2) {

            System.out.println(a[i]);
        }
    }

    public static void main(String[] args) {
        array a = new array();
        a.m();
    }
}
