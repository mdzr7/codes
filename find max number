package payilagam;

public class max_element {
    public static void main(String[] args) {
        int a[] = {7, 5, 4, 9, 3, 2};
        int max = a[0];

        for (int i = 1; i < a.length; i = i + 1) {
            if (a[i] > max) {
                max = a[i];
            }
        }
        System.out.println(max);
    }
}
