//merge two array into one or single array

public class two_array {
    public static void main(String[] args) {

        int a[] = {7, 4, 1, 5, 3};
        int b[] = {11, 14, 3, 2, 9};
        int c[] = new int[a.length + b.length];

        for (int i = 0; i < a.length; i++) {
            c[i] = a[i];
        }
        for (int i = 0; i < a.length; i++) {
            c[i + a.length] = b[i];
        }
        for (int i : c){
            System.out.print(i + " , ");
        }

    }
}
