package payilagam.array;

public class min_element {
    public static void main(String[] args) {
        int a[] = {7, 5, 4, 9, 3, 2};
        int min = a[0];

        for (int i = 1; i < a.length; i = i + 1) {
            if (a[i] < min) {
                min = a[i];
            }
        }
        System.out.println(min);
    }
}
